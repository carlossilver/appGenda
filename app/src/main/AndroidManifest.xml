<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mxdigitalacademy.appgenda">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppGenda">
        <activity android:name=".actividades.Editor"></activity>
        <activity android:name=".actividades.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".actividades.InfoContacto"
            android:parentActivityName=".actividades.MainActivity" />
        <activity
            android:name=".actividades.NewContact"
            android:parentActivityName=".actividades.MainActivity" />
    </application>

</manifest>